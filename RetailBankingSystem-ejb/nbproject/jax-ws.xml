<?xml version="1.0" encoding="UTF-8"?>
<jax-ws xmlns="http://www.netbeans.org/ns/jax-ws/1">
  <services>
    <service name="MerlionBankWebService">
      <implementation-class>ejb.ws.session.MerlionBankWebService</implementation-class>
    </service>
    <service name="FastTransferWebService">
      <implementation-class>ejb.ws.session.FastTransferWebService</implementation-class>
    </service>
  </services>
  <clients>
    <client name="OtherBanksWebService">
      <wsdl-url>http://localhost:8080/OtherBanksWebService/OtherBanksWebService?wsdl</wsdl-url>
      <local-wsdl-file>localhost_8080/OtherBanksWebService/OtherBanksWebService.wsdl</local-wsdl-file>
      <package-name forceReplace="true">ws.client.otherbanks</package-name>
      <catalog-file>catalog.xml</catalog-file>
      <wsimport-options>
        <wsimport-option>
          <wsimport-option-name>extension</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>verbose</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>fork</wsimport-option-name>
          <wsimport-option-value>false</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>wsdlLocation</wsimport-option-name>
          <wsimport-option-value>http://localhost:8080/OtherBanksWebService/OtherBanksWebService?wsdl</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xnocompile</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>xendorsed</wsimport-option-name>
          <wsimport-option-value>true</wsimport-option-value>
        </wsimport-option>
        <wsimport-option>
          <wsimport-option-name>package</wsimport-option-name>
          <wsimport-option-value>ws.client.otherbanks</wsimport-option-value>
        </wsimport-option>
      </wsimport-options>
    </client>
  </clients>
</jax-ws>
